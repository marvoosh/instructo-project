<div class="navbar navbar-expand-sm navbar-light navbar-lewagon">
  <%= link_to root_path, class: "navbar-brand" do %>
    <%= image_tag "logo.png" %>
  <% end %>

  <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
    <span class="navbar-toggler-icon"></span>
  </button>


  <div class="collapse navbar-collapse" id="navbarSupportedContent">
    <ul class="navbar-nav mr-auto">
      <% if user_signed_in? %>

        <li class="nav-item">
          <%= link_to "About Us", "#", class: "nav-link" %>
        </li>

        <li class="nav-item">
          <%= link_to "Find Instructor", instructors_path, class: "nav-link" %>
        </li>
        <li class="nav-item">
          <%= link_to "Become an Instructor", new_instructor_path, class: "nav-link" %>
        </li>
        <li class="nav-item dropdown">
          <% if current_user.profile_url == nil %>
            <%= image_tag "data:image/png;base64,", class: "avatar dropdown-toggle", id: "navbarDropdown", data: { toggle: "dropdown" }, 'aria-haspopup': true, 'aria-expanded': false %>
          <% else %>
            <%= image_tag current_user.profile_url, class: "avatar dropdown-toggle", id: "navbarDropdown", data: { toggle: "dropdown" }, 'aria-haspopup': true, 'aria-expanded': false %>
          <% end %>
          <div class="dropdown-menu dropdown-menu-right" aria-labelledby="navbarDropdown">
              <h6><strong><%= current_user.first_name %> <%= current_user.last_name %></strong></h6>
            <% if current_user.instructor %>
              <%= link_to "Teaching - my bookings", instructor_bookings_path(current_user), class: "dropdown-item" %>
              <%= link_to "Learning - my bookings", dashboard_path(current_user), class: "dropdown-item" %>
            <% else %>
              <%= link_to "My bookings", dashboard_path(current_user), class: "dropdown-item" %>
            <% end %>
              <%= link_to "Log out", destroy_user_session_path, method: :delete, class: "dropdown-item" %>
          </div>
        </li>
      <% else %>
        <li class="nav-item">
          <%= link_to "Login", new_user_session_path, class: "nav-link" %>
        </li>
      <% end %>
    </ul>
  </div>
</div>
